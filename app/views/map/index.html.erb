<% provide(:title, 'Map') %>
<% content_for :head do %>
  <script src="https://maps.google.com/maps/api/js?key=AIzaSyCLAH2-kZr051VrKMhGFzRQ22QsPH_aooU&libraries=places"></script>
  <script src="https://cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
<% end %>
<form class="map-form">
  <input id="geocomplete" type="text" placeholder="Type in an address" size="90" />
  <input id="find" type="button" value="find" />
</form>
<div id="map" class="map">

</div>
